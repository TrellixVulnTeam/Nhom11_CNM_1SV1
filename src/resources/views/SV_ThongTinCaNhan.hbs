    <div class="main-thong-tin-sv col">
      {{! /* Thông tin học vấn */ }}
      <div class="thong-tin-hoc-van row">
        <div class="nav anh-sinh-vien col col-lg-3">
            <img src="https://scontent.fdad3-4.fna.fbcdn.net/v/t1.6435-9/125804649_1537388186446618_1908998491894744380_n.jpg?_nc_cat=101&ccb=1-5&_nc_sid=09cbfe&_nc_ohc=fWPNR0pibKMAX9MsQ95&tn=4shL0tcfUZx9fRYh&_nc_ht=scontent.fdad3-4.fna&oh=00_AT_mPbnffoKAxRdDAuq2ApriWvhYkbugDJfGscLfH3s4xw&oe=61E62BEA" 
            alt="ảnh thẻ sinh viên" class="img-reponsive">
          <div class="rol">
            <div class="form-group">
              <div class="control-label">
                <span lang="thongtinsinhvien-mssv">MSSV</span>:
                <b>{{ttall.MaSinhVien}}</b>
              </div>
            </div>
            <div class="form-group">
              <div class="control-label">
                <span lang="thongtinsinhvien-hovaten">Họ tên</span>:
                <b>{{ttall.TenSinhVien}}</b>
              </div>
            </div>
            <div class="form-group">
              <div class="control-label">
                <span lang="thongtinsinhvien-gioitinh">Giới tính</span>:
                <b>{{ttall.GioiTinh}}</b>
              </div>
              <div class="clearfix"></div>
            </div>
          </div>
        </div>
        <div class="col-lg-9">
          <div class="text-title">
            <p>
              Thông tin học vấn
            </p>
            <hr style="width:100%;color:#cccccc" />
          </div>
          <div class="main-thong-tin">
            <div class="col-md-12">
              <form class="form-horizontal">
                <div class="form-body">
                  <div class="form-group">
                    <label class="col-md-3"><span lang="thongtinsinhvien-trangthai">Trạng thái</span>:
                      <span class="bold"> {{ttall.TrangThai}} </span></label>
                    <label class="col-md-3"><span lang="thongtinsinhvien-mahoso">Mã hồ sơ</span>:
                      <span class="bold"></span></label>
                    <label class=""><span lang="thongtinsinhvien-ngayvaotruong">Ngày vào trường</span>:
                      <span class="bold">{{ngay ttall.NgayVaoTruong}}</span></label>
                  </div>
                  <div class="form-group">
                    <label class="col-md-6"><span lang="thongtinsinhvien-lophoc">Lớp học</span>:
                      <span class="bold">{{ttall.TenLop}}</span></label>
                    <label class=""><span lang="thongtinsinhvien-coso">Cơ sở</span>:
                      <span class="bold">{{ttall.CoSo}}</span></label>
                  </div>
                  <div class="form-group">
                    <label class="col-md-6"><span lang="thongtinsinhvien-bacdaotao">Bậc đào tạo</span>:
                      <span class="bold">{{ttall.BacDaoTao}}</span></label>
                    <label class=""><span lang="thongtinsinhvien-loaidaotao">Loại hình đào tạo</span>:
                      <span class="bold">{{ttall.LoaiHinhDaotao}}</span></label>
                  </div>
                  <div class="form-group">
                    <label class="col-md-6"><span lang="thongtinsinhvien-khoa">Khoa</span>:
                      <span class="bold">{{ttall.TenKhoa}}</span></label>
                    <label class=""><span lang="thongtinsinhvien-nganh">Ngành</span>:
                      <span class="bold">{{ttall.TenChuyenNganh}}
                      </span></label>
                  </div>
                  <div class="form-group">
                    <label class="col-md-6"><span lang="thongtinsinhvien-chuyennganh">Chuyên ngành</span>:
                      <span class="bold">{{ttall.TenChuyenNganh}} - 7480103</span></label>
                    <label class=""><span lang="thongtinsinhvien-khoahoc">Khóa
                        học</span>:
                      <span class="bold">2018 - 2019</span></label>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>

      {{! /* Thông tin cá nhân */ }}
      <div class="thong-tin-ca-nhan">
        <div class="text-title">
          <p>
            Thông tin cá nhân
          </p>
          <hr style="width:100%;color:#cccccc" />
        </div>
        <div class="main-ca-nhan">
          <div class="form-horizontal">
            <div class="form-body">
              <div class="col-md-12">
                <div class="form-group">
                  <label class="col-md-3"><span lang="thongtinsinhvien-ngaysinh">Ngày sinh</span>:
                    <span class="bold">{{ngay ttall.NamSinh}}</span></label>
                  <label class="col-md-3"><span lang="thongtinsinhvien-dantoc">Dân tộc</span>:
                    <span class="bold">{{ttall.DanToc}}</span></label>
                  <label class="col-md-3"><span lang="thongtinsinhvien-tongiao">Tôn giáo</span>:
                    <span class="bold">{{ttall.TonGiao}}</span></label>
                  <label class=""><span lang="thongtinsinhvien-khuvuc">Khu vực</span>:
                    <span class="bold">{{ttall.KhuVuc}}</span></label>
